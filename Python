class PetCat(object):

    def __init__(self,name,age):


        self.name=name
        self.age=age

    def show_inof(self):
        return '我叫{0},今年{1}岁'.format(self.name,self.age)

    def __str__(self):
        return self.show_inof()

if __name__=='__main__':
    cat_black=PetCat('小黑',2)
    print(cat_black)


class Person(object):

    def __init__(self,name,gender):
        self.name=name
        self.gender=gender

    def speak(self):
        print("hello ! 我是{0}".format(self.name))

    def relation(self):
        pass

class Student(Person):

    def __init__(self,name,gender,score,major):
        Person(name,gender)
        self.score=score
        self.major=major

class BaseCat(object):
    tag='猫科动物'

    def __init__(self,name):
        self.name=name

    def eat(self):
        print('猫懂要吃东西')


class Tiger(BaseCat):
    def eat(self):
        super(Tiger,self).eat()
        print("我还喜欢吃肉")
class Panda(BaseCat):
    pass

class PetCat(BaseCat):
    def eat(self):
        super(PetCat,self).eat()
        print("我还喜欢猫粮")

class HuaCat(PetCat):
    def eat(self):
        super(HuaCat,self).eat()
        print("我还喜欢零食")

if __name__=='__main__':
    cat=HuaCat('小黄')
    cat.eat()


