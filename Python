import matplotlib.pyplot as plt
# x_values=[1,2,3,4,5]
# y_values=[1,4,9,16,25]
x_values=list(range(1,1001))
y_values=[x**2 for x in x_values]
plt.scatter(x_values,y_values,s=100)
plt.title("Square Numbers",fontsize=24)
plt.xlabel("Value",fontsize=14)
plt.tick_params(axis='both',which='major',labelsize=14)
plt.axis([0,1100,0,1100000])
plt.show()
import pymysql
import matplotlib.pyplot

squares = [1, 4, 9, 16, 25]
matplotlib.pyplot.plot(squares, linewidth=5)
matplotlib.pyplot.title("Square Numbers", fontsize=24)
matplotlib.pyplot.xlabel("Value", fontsize=14)
matplotlib.pyplot.ylabel("Square of Value", fontsize=14)
matplotlib.pyplot.tick_params(axis='both', labelsize=14)

matplotlib.pyplot.show()


class Mysql:
    def __init__(self):
        self.conn = pymysql.connect(host="localhost", port=3306, user="root", passwd="123456", db="student",
                                    charset="UTF8")
        self.cursor = self.conn.cursor()

    def selectStudent(self):
        sqlstr = 'SELECT * FROM st_info'
        self.cursor.execute(sqlstr)
        data = self.cursor.fetchall()
        for d in data:
            print('sid:{0} \t sName:{1} \t sAge:{2}'.format(d[0], d[1], d[2]))

    def insertStudent(self):
        name = input("请输入插入的name:")
        age = int(input("请输入插入的age:"))
        sqlstr = 'INSERT into st_info VALUES (default,"%s",%d)'%(name,age)
        self.cursor.execute(sqlstr)
        self.conn.commit()
    def deleteStudent(self):
        id = int(input("请输入删除对象的id:"))
        sqlstr = 'DELETE FROM st_info WHERE sId=%d'%(id)
        self.cursor.execute(sqlstr)
        self.conn.commit()
    def updateStudent(self):
        id=int(input("请输入删改对象的id:"))
        name=input("请输入修改后的name:")
        age=int(input("请输入修改后的age:"))
        sqlstr='UPDATE st_info SET sName="%s",sAge=%d WHERE sId=%d'%(name,age,id)
        self.cursor.execute(sqlstr)
        self.conn.commit()

    def closeStudent(self):
        self.conn.close()
from mysql import Mysql


def title():
    print("*************************请选择序号*************************")
    print("1.查询数据")
    print("2.修改数据")
    print("3.插入数据")
    print("4.删除数据")
    print("0.退出")
    print("*************************---end---*************************")

title()
sq=Mysql()
i=''
while i!='0':
    i=input('请输入序号：')
    if i=='1':
        sq.selectStudent()
    elif i=='2':
        sq.updateStudent()
    elif i=='3':
        sq.insertStudent()
    elif i=='4':
        sq.deleteStudent()
    elif i=='0':
        print("系统已退出")
    else:
        print("输入错误请重新输入")
        title()

sq.closeStudent()
