service_menu={1:"人民币转换美元",2:"美元转换人民币",3:"人民币转换欧元",0:"结束程序"}
fw=5
while fw !=0:
    print("**********欢迎使用货币转换服务系统**********")
    print('1:',service_menu[1])
    print('2:',service_menu[2])
    fw=int(fw)
    while fw != 0:
        if fw == 1:
            print("欢迎使用人民币转换美元服务")
            a=input("请输入您需求转换的人民币金额：")
            print("您需要转换的人民币为：",a)
            print("兑换成美元为：{0}$".format(int(a)*7.0))
            break
        elif fw ==2:
            print("欢迎使用美元转换人民币服务")
            a = input("请输入您需求转换的美元金额：")
            print("您需要转换的美元为：", a)
            print("兑换成人民币为：{0}$".format(int(a) / 7.0))
            break
        elif fw ==3:
            print("欢迎使用人民币转换欧元服务")
            a = input("请输入您需求转换的人民币金额：")
            print("您需要转换的人民币为：", a)
            print("兑换成欧元为：{0}$".format(int(a) / 10.0))
            break
    print('3:',service_menu[3])
    print('0:',service_menu[0])
    fw=input("请您选择需要的服务：")
if fw==0:
    print("感谢您的使用，祝您生活愉快，再见！")
