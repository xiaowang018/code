# 登录
chrone_driver = r"C:\\Users\\30452\\Desktop\\zdh\\chromedriver.exe"

driver = webdriver.Chrome(executable_path=chrone_driver)
driver.get("http://172.26.101.56/bsams/front/login.do")
sleep(1)
driver.find_element_by_id("taskId").send_keys("109")
driver.find_element_by_id("loginName").send_keys("201937050211")
driver.find_element_by_id("password").send_keys("123456")
driver.find_element_by_id("vericode").send_keys("shtd")
driver.find_element_by_class_name("dl-button").click()
