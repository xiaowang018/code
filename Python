import time
error_count = []
di = []
lock_name = []
lock_di = []


def passs(LoginName, Password):
    if error_count.count(LoginName) < 3 and not LoginName + '\n' in lock_di:
        if 'LoginName:' + LoginName + '\n' in di:
            i = di.index('LoginName:' + LoginName + '\n')
            if 'Password:' + Password + '\n' == di[i + 1]:
                print('登录成功')
            else:
                print('密码或用户名错误')
                time60()
                error_count.append(LoginName)
        else:
            print('密码或用户名错误')
            time60()
            error_count.append(LoginName)
    else:
        lock_name.append(LoginName + '\n')
        print(LoginName + '已锁定')


def time60():
    i = 60
    while i:
        print(i, end='')
        print('秒后重试')
        i = i - 1
        # time.sleep(1)


while True:
    LoginName = input('请输入用户名：')
    Password = input('请输入密码：')
    fp = open('login.txt')
    fp2 = open('log2.txt')
    fp3 = open('log2.txt', 'a')
    lock_di = fp2.readlines()
    di = fp.readlines()
    passs(LoginName, Password)
    fp3.writelines(lock_name)
    fp.close()
    fp2.close()
    fp3.close()

class Fruit:
    fName = ''
    fType = ''

    def __init__(self, name, type):
        self.fName = name
        self.fType = type

    def FIntroduce(self):
        print('我是%s，类型是%s水果' % (self.fName, self.fType))


class Vegetables:
    vName = ''
    vType = ''

    def __init__(self, name, type):
        self.vName = name
        self.vType = type

    def VIntroduce(self):
        print('我是%s，类型是%s蔬菜' % (self.vName, self.vType))


class Tomato(Fruit, Vegetables):
    colour = ''
    weight = 0

    def __init__(self,name,ftype,vtype,colour,weight):
        Fruit.fName=name
        Fruit.fType=ftype
        Vegetables.vName=name
        Vegetables.vType=vtype
        self.colour=colour
        self.weight=weight

    def TIntroduce(self):
        Fruit.FIntroduce(self)
        Vegetables.VIntroduce(self)
        print('颜色为%s重量为%dg'%(self.colour,self.weight))



--------------
class Circle:
    rad=0
    Cper = 0
    Carea = 0
    def __init__(self,rad):
        self.rad=rad

    def CPer(self):
        self.Cper=self.rad*2*3.14

    def CArea(self):
        self.Carea=3.14*self.rad*self.rad

class Square:
    length=0
    wide=0
    Sper = 0
    Sarea = 0
    def __init__(self,length,wide):
        self.length=length
        self.wide=wide

    def SPer(self):
        self.Sper=(self.length+self.wide)*2

    def SArea(self):
        self.Sarea=self.wide*self.length

class shade(Circle,Square):
    sper=0
    sarea=0

    def __init__(self,rad,length,wide):
        Circle.__init__(self,rad)
        Square.__init__(self,length,wide)
        self.CPer()
        self.CArea()
        self.SArea()
        self.CArea()

    def sPer(self):
        self.sper=self.Cper+self.Carea
        return self.sper

    def sArea(self):
        self.sarea=self.Carea-self.Sarea
        return self.sarea

