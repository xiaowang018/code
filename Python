def print_verse(verse_name,is_show_title):
    if verse_name=="咏鹅":
        if is_show_title==True:
            print("咏鹅")
        print("鹅鹅鹅，曲项向天歌，白毛浮绿水，红掌拨清波。")
    elif verse_name =="悯农":
        if is_show_title==True:
            print("悯农")
        print("锄禾日当午")
        print("汗滴禾下土")
        print("谁知盘中餐")
        print("粒粒皆辛苦")

print_verse("悯农",1)
def oper(num,num1,num2):
    if num<100:
        print(num1*num2)
    else:
        print(num1+num2)
