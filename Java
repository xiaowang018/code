#include<stdio.h>
int main(){
	int a,i,max;
    scanf("%d",&a);
    int s[a];
    max=0;
    for(i=0;i<a;i++){
    scanf("%d",&s[i]);
    if(s[i]>max)
        max=s[i];
    }
    printf("%d",max);
} 
#include<stdio.h>
#define s 60
int main(){
	int h=0,f;
	do{scanf("%d",&f);
	while(f>=s){
		h++;f-s;
	}
	printf("%d小时 %d分钟",h,f);
	}while(f>0);
} 
