package com.bug;

public class yf extends kxgz{
    public yf(String name,int shu,int bug){
        setName(name);
        setShu(shu);
        setBug(bug);
    }
    public void sca(){
        System.out.println(this.getName()+"工作类信息测试："+this.getName()+
                "工作的日报是：今天编写了"+this.getShu()+"行代码，目前仍然有"
                +this.getBug() +"个bug没有解决");
    }


}

package com.bug;

public class cs extends kxgz {

    public cs(String name,int shu,int bug){
        setName(name);
        setShu(shu);
        setBug(bug);
    }
    public void sca(){
        System.out.println(this.getName()+"工作类信息测试："+this.getName()+
                "工作的日报是：今天编写了"+this.getShu()+"个测试用例，发现了"
                +this.getBug() +"个bug");
    }

}

package com.bug;

public class kxgz {
    private String name;
    private int shu;
    private int bug;

    public void setName(String name) {
        this.name = name;
    }

    public void setBug(int bug) {
        this.bug = bug;
    }

    public void setShu(int shu) {
        this.shu = shu;
    }

    public String getName() {
        return name;
    }

    public int getBug() {
        return bug;
    }

    public int getShu() {
        return shu;
    }
}

